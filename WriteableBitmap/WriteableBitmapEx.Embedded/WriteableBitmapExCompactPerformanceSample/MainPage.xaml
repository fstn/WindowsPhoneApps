<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	x:Class="WBEX.SWE.Performance.MainPage">
    <UserControl.Resources>
    </UserControl.Resources>

    <Grid x:Name="LayoutRoot" Background="White">
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>
        <Grid x:Name="ViewPortContainer" Height="150" Width="300" Grid.Row="0">
            <Image x:Name="ImageViewport" />
            <Rectangle Stroke="Red" />
        </Grid>
            <StackPanel Grid.Row="2" Orientation="Horizontal" Background="White" >
                <!--<TextBlock Foreground="Black" Text="No. of shapes:"/>-->
                <TextBox x:Name="TxtBoxShapeCount" Text="10" Width="100"  Foreground="Black" BorderBrush="Black" />
                <TextBlock Margin="8" x:Name="TxtBlockPerf"  Width="100" Text="Speed" Foreground="Black" />
            </StackPanel>
    </Grid>
</UserControl>